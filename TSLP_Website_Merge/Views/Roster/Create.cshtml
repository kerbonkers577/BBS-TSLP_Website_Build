@model TSLP_Website_Merge.Models.Roster

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Roster</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="check" /> @Html.DisplayNameFor(model => model.check)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="sellerId" class="control-label"></label>
                <input asp-for="sellerId" class="form-control" />
                <span asp-validation-for="sellerId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="rosterDate" class="control-label"></label>
                <input asp-for="rosterDate" class="form-control" />
                <span asp-validation-for="rosterDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

