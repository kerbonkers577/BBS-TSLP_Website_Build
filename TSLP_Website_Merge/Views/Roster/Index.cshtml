@model IEnumerable<TSLP_Website_Merge.Models.Roster>

@{
    ViewData["Title"] = "Index";
}

<h1>Roster</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.check)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sellerId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.rosterDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.check)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sellerId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.rosterDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
